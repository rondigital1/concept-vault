{
  "mcpServers": {
    "x-bookmarks": {
      "command": "npx",
      "args": ["tsx", "server/mcp/x-bookmarks/index.ts"],
      "env": {
        "DATABASE_URL": "${DATABASE_URL}",
        "X_CLIENT_ID": "${X_CLIENT_ID}",
        "X_ACCESS_TOKEN": "${X_ACCESS_TOKEN}",
        "X_REFRESH_TOKEN": "${X_REFRESH_TOKEN}"
      }
    }
  }
}
